{
  "name": "skartner-admin",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn codegen:watch\" \"vite --port 4173\"",
    "build": "vite build",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "nodemon -w src -e graphql,gql --exec \"yarn codegen\"",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "firebase": "^10.5.2",
    "firebaseui": "^6.1.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.13.1",
    "ra-data-json-server": "^4.9.2",
    "react": "^18.2.0",
    "react-admin": "^4.9.0",
    "react-admin-firebase": "^4.1.3",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^2.2.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0"
  }
}
